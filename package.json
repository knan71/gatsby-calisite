    "gatsby-plugin-typography": "^2.3.12",
    "gatsby-remark-embed-youtube": "0.0.7",
    "gatsby-remark-emojis": "^0.3.3",
    "gatsby-remark-images": "^3.1.28",
    "gatsby-remark-relative-images": "^0.2.3",
    "gatsby-remark-responsive-iframe": "^2.2.25",
    "gatsby-remark-wrap-iframe": "^1.1.0",
    "gatsby-source-filesystem": "^2.1.29",
    "gatsby-transformer-remark": "^2.6.27",
    "gatsby-transformer-sharp": "^2.3.1",
